	background-color: rgb(150, 0, 0);
	min-width: 30px;
	border-radius: 5px;
}
QScrollBar::handle:horizontal:hover {
	background-color: rgb(170, 0, 0);
}

QScrollBar::handle:horizontal:pressed {
	background-color: rgb(140, 0, 0);
}








/*BTN LEFT, RIGHT - SCROLLBAR*/
